{
  "extends": ["react-app", "prettier", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warning",
    "react/no-unescaped-entities": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "react/no-unknown-property": 0,
    "import/no-anonymous-default-export": 0
  }
}
